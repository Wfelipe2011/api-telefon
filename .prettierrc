{
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"tabWidth": 2,
	"printWidth": 140,
	"proseWrap": "never",
	"trailingComma": "es5",
	"bracketSameLine": false,
	"singleAttributePerLine": false,
	"useTabs": true,
	"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./@]", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
